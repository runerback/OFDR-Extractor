<Application x:Class="Extractor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             DispatcherUnhandledException="onDispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Themes/Images.xaml" />
                <ResourceDictionary Source="/Themes/Colors.xaml" />
                <ResourceDictionary Source="/Themes/ButtonTheme.xaml" />
                <ResourceDictionary Source="/Themes/ProgressBarTheme.xaml" />
                <ResourceDictionary Source="/Themes/CheckBoxTheme.xaml" />
                <ResourceDictionary Source="/Themes/ToggleButtonTheme.xaml" />
                <ResourceDictionary Source="/Themes/CommonTheme.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
